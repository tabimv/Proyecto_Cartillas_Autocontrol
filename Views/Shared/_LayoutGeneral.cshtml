                        <p><i class="fa fa-phone" style="color:#f36f21;"></i> +56 9 6208 7187</p>
                        <p><i class="fa fa-globe" style="color:#f36f21;"></i> www.paumar.cl</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/Content/js/menu.js"></script>
    <script src="~/Content/js/CRUD.js"></script>
    @RenderSection("js", required: false)
